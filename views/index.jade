extends ../layouts/base

block js
    script(src='js/vendor/jquery.qtip.js')
    script(src='js/vendor/xdate.js')
    script(src='js/vendor/underscore.js')
    script(src='js/vendor/backbone.js')
    script(src='js/vendor/Chart.js')
    script(src='js/main.js')

block content
    .latencies.row
        .allControlsContainer.columns
        .legendContainer.columns
        .main.columns
            .downtimeContainer
                .info
                    h2 downtime
                    .help Downtime in minutes. The lower, the better.
            .latencyContainer
                .info
                    h2 latency
                    .help Latency is given as average for the defined range of time. The lower, the better.
